    "@holo-host/mock-conductor": "^0.3.3",
    "@holochain-open-dev/core-types": "^0.2.1",
    "@innei-util/eslint-config-react-ts": "^0.1.2",
    "@innei-util/prettier": "0.8.2",
    "@jest/types": "^28.1.1",
    "@storybook/addon-actions": "6.5.9",
