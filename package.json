{
  "name": "vite-react-tailwind-template",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "format": "prettier --write \"src/**/*.ts*\" ",
    "lint": "eslint ./src --fix",
    "test": "jest",
    "gen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,less}": [
      "prettier --ignore-path ./.prettierignore --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@holochain/client": "0.4.1",
    "@reduxjs/toolkit": "^1.8.0",
    "formik": "^2.2.9",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.12.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.3.0",
    "redis": "^4.0.4",
    "ws": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.17.12",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-operations": "2.3.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.8",
    "@innei-util/eslint-config-react-ts": "^0.1.2",
    "@innei-util/prettier": "^0.1.3",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/react": "^6.5.8",
    "@storybook/testing-library": "^0.0.11",
    "@types/jest": "^28.1.1",
    "@types/react": "17.0.20",
    "@types/react-dom": "^17.0.13",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "autoprefixer": "10.3.4",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.32.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "7.0.2",
    "jest": "^28.1.1",
    "lint-staged": "11.1.2",
    "postcss": "8.4.14",
    "postcss-nested": "5.0.6",
    "prettier": "2.4.0",
    "react-is": ">= 16.8.0",
    "storybook": "^6.5.8",
    "styled-components": "^5.3.3",
    "tailwindcss": "2.2.15",
    "typescript": "4.4.3",
    "vite": "2.5.6",
    "vite-plugin-checker": "0.3.4",
    "vite-tsconfig-paths": "3.3.14"
  }
}
