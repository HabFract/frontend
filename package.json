{
  "name": "vite-react-tailwind-template",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "npm run watch:css & vite",
    "c-dev": "concurrently \"npm run dev\" \"npm run storybook -- --no-manager-cache --quiet\" \"npm run gen\"",
    "build": "npm run build:css tsc && vite build",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/styles.css",
    "watch:css": "TAILWIND_MODE=watch npx tailwindcss -c ./tailwind.config.js --postcss -i src/assets/tailwind.css -o src/assets/styles.css --watch",
    "serve": "vite preview",
    "format": "prettier --write \"src/**/*.ts*\" ",
    "lint": "eslint ./src --fix",
    "test": "jest --clearCache",
    "gen": "graphql-codegen --config codegen.yml --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,less}": [
      "prettier --ignore-path ./.prettierignore --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "3.6.6",
    "@holochain/client": "0.4.1",
    "@reduxjs/toolkit": "^1.8.0",
    "antd": "^4.21.0",
    "formik": "^2.2.9",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.12.6",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.3.0",
    "redis": "^4.0.4",
    "ws": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.17.12",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-operations": "2.3.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.8",
    "@innei-util/eslint-config-react-ts": "^0.1.2",
    "@innei-util/prettier": "^0.1.3",
    "@jest/types": "^28.1.1",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^28.1.1",
    "@types/react": "18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.3.2",
    "@zougt/some-loader-utils": "^1.4.3",
    "@zougt/vite-plugin-theme-preprocessor": "^1.4.5",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.2.2",
    "css-loader": "^5.2.7",
    "eslint": "7.32.0",
    "eslint-plugin-storybook": "^0.5.12",
    "graphql-codegen-typescript-mock-data": "^2.2.3",
    "graphql-scalars": "^1.17.0",
    "html-webpack-plugin": "^4",
    "husky": "7.0.2",
    "jest": "^28.1.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "11.1.2",
    "node-sass": "^7.0.1",
    "postcss": "8.3.6",
    "postcss-cli": "^9.1.0",
    "postcss-nested": "5.0.6",
    "postcss-scss": "^4.0.4",
    "prettier": "2.4.0",
    "react-is": ">= 16.8.0",
    "require-from-string": "^2.0.2",
    "sass": "^1.52.3",
    "sass-loader": "^10.0.0",
    "storybook": "6.5.9",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "style-loader": "^2.0.0",
    "styled-components": "^5.3.3",
    "tailwindcss": "^2.2.15",
    "ts-loader": "^6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.4.3",
    "typescript-plugin-styled-components": "^2.0.0",
    "vite": "^2.9.12",
    "vite-plugin-checker": "0.3.4",
    "vite-tsconfig-paths": "3.3.14"
  }
}
