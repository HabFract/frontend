{
  "name": "vite-react-tailwind-template",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "concurrently \"npm run watch:css\" \"vite\"",
    "c-dev": "concurrently \"npm run dev\" \"npm run storybook -- --no-manager-cache --quiet\" \"npm run gen\"",
    "build": "npm run build:css tsc && vite build",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/styles.css",
    "watch:css": "TAILWIND_MODE=watch npx tailwindcss -c ./tailwind.config.js --postcss -i src/assets/tailwind.css -o src/assets/styles.css --watch",
    "serve": "vite preview",
    "format": "prettier --write --ignore-path ./.prettierignore \"src/**/*.ts*\" \"src/**/*.tsx*\" \"src/**/*.graphql\" ",
    "lint": "eslint ./src --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch -no-cache --verbose --runInBand --config=./jest.config.ts",
    "prepush": "jest -no-cache --verbose --runInBand --config=./jest.config.ts",
    "gen": "graphql-codegen --config codegen.yml --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test || exit 0"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,less}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "3.6.6",
    "@graphql-tools/schema": "^8.3.14",
    "@holochain/client": "0.4.1",
    "@reduxjs/toolkit": "1.8.2",
    "antd": "4.22.8",
    "buffer": "^6.0.3",
    "deep-for-each": "^3.0.0",
    "fecha": "^4.2.3",
    "formik": "2.2.9",
    "graphql": "15.8.0",
    "graphql-tag": "^2.12.6",
    "is-object": "^1.0.2",
    "js-base64": "^3.7.2",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "redis": "^4.1.0",
    "ws": "*",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
